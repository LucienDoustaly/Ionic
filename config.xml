<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.isen.ionic.isenenslip" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>IsenEnSlip</name>
    <description>Juste l'ISEN en Slip</description>
    <author email="wilfried.allart@isen.yncrea.fr">Team Slip</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="Orientation" value="landscape" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/drawable-ldpi/icon.png" />
        <icon density="mdpi" src="resources/android/drawable-mdpi/icon.png" />
        <icon density="hdpi" src="resources/android/drawable-hdpi/icon.png" />
        <icon density="xhdpi" src="resources/android/drawable-xhdpi/icon.png" />
        <icon density="xxhdpi" src="resources/android/drawable-xxhdpi/icon.png" />
        <icon density="xxxhdpi" src="resources/android/drawable-xxxhdpi/icon.png" />
        <splash density="land-ldpi" src="resources/android/drawable-land-ldpi/screen.png" />
        <splash density="land-mdpi" src="resources/android/drawable-land-mdpi/screen.png" />
        <splash density="land-hdpi" src="resources/android/drawable-land-hdpi/screen.png" />
        <splash density="land-xhdpi" src="resources/android/drawable-land-xhdpi/screen.png" />
        <splash density="land-xxhdpi" src="resources/android/drawable-land-xxhdpi/screen.png" />
        <splash density="land-xxxhdpi" src="resources/android/drawable-land-xxxhdpi/screen.png" />
        <splash density="port-ldpi" src="resources/android/drawable-ldpi/screen.png" />
        <splash density="port-mdpi" src="resources/android/drawable-mdpi/screen.png" />
        <splash density="port-hdpi" src="resources/android/drawable-hdpi/screen.png" />
        <splash density="port-xhdpi" src="resources/android/drawable-xhdpi/screen.png" />
        <splash density="port-xxhdpi" src="resources/android/drawable-xxhdpi/screen.png" />
        <splash density="port-xxxhdpi" src="resources/android/drawable-xxxhdpi/screen.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="20" src="resources/ios/AppIcon.appiconset/Icon-20x20@3x.png" width="20" />
        <icon height="29" src="resources/ios/AppIcon.appiconset/Icon-29x29@3x.png" width="20" />
        <icon height="40" src="resources/ios/AppIcon.appiconset/Icon-40x40@3x.png" width="40" />
        <icon height="60" src="resources/ios/AppIcon.appiconset/Icon-60x60@3x.png" width="60" />
        <icon height="76" src="resources/ios/AppIcon.appiconset/Icon-76x76@3x.png" width="76" />
        <splash height="1136" src="resources/ios/LaunchImage.launchimage/LaunchImage-568h@2x~iphone_640x1136.png" width="568" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-device" spec="^2.0.1" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="^1.1.16" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.0.5" />
    <plugin name="cordova-plugin-screen-orientation" spec="^3.0.1" />
    <plugin name="cordova-plugin-nativeaudio" spec="^3.0.9" />
    <plugin name="cordova-plugin-filechooser" spec="^1.0.1" />
    <plugin name="cordova-plugin-x-toast" spec="^2.6.2" />
    <plugin name="cordova-plugin-media" spec="^5.0.2" />
    <plugin name="cordova-plugin-vibration" spec="^3.0.1" />
    <plugin name="cordova-plugin-android-permissions" spec="^1.0.0" />
    <plugin name="cordova.plugins.diagnostic" spec="^4.0.3" />
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
    <plugin name="cordova-plugin-filepath" spec="^1.2.0" />
    <plugin name="cordova-plugin-background-mode" spec="^0.7.2" />
    <engine name="android" spec="7.0.0" />
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
</widget>
